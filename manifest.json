{
    "_version": "1.12.0",
    "sap.app": {
        "id": "REPINTEMPOLEEY.REPINTEMPOLEEY",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.40.12"
        },
        "dataSources": {
            "mainService": {
                "uri": "/HANAMDC/REPINT/RepintEmployee/xsodata/RepintEmployee.xsodata/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "REPINTEMPOLEEY.REPINTEMPOLEEY.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.65.6",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "REPINTEMPOLEEY.REPINTEMPOLEEY.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "REPINTEMPOLEEY.REPINTEMPOLEEY.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "SchedaRepInt",
                    "pattern": "",
                    "target": [
                        "SchedaRepInt"
                    ]
                },
                {
                    "name": "Unauthorized",
                    "pattern": "Unauthorized",
                    "target": [
                        "Unauthorized"
                    ]
                }
            ],
            "targets": {
                "SchedaRepInt": {
                    "viewType": "XML",
                    "viewName": "SchedaRepInt"
                },
                "Unauthorized": {
                    "viewType": "XML",
                    "viewName": "Unauthorized"
                }
            }
        }
    },
    "sap.platform.hcp": {
        "uri": "webapp",
        "_version": "1.1.0"
    }
}